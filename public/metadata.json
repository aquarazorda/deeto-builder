[
  {
    "type": "group",
    "title": "Images",
    "description": "Here you can choose logo or background image",
    "elements": [
      {
        "type": "item",
        "behaviour": "image",
        "title": "Logo",
        "defaultValue": "/images/placeholder-logo.svg",
        "selectors": ["header img"]
      },
      {
        "type": "item",
        "behaviour": "background",
        "title": "Page background",
        "defaultValue": "#041527",
        "selectors": ["container"]
      }
    ]
  },
  {
    "type": "group",
    "title": "Fonts",
    "description": "Here you can choose fonts",
    "elements": [
      {
        "type": "group",
        "elements": [
          {
            "type": "item",
            "behaviour": "font",
            "defaultValue": "Inter",
            "title": "Inter",
            "selectors": ["container"]
          },
          {
            "type": "item",
            "behaviour": "font",
            "defaultValue": "DM Sans",
            "title": "DM Sans",
            "selectors": ["container"]
          }
        ]
      }
    ]
  },
  {
    "type": "group",
    "title": "Colors",
    "description": "Here you can choose colors",
    "elements": [
      {
        "type": "group",
        "title": "Texts",
        "elements": [
          {
            "type": "item",
            "behaviour": "color",
            "title": "Text #1",
            "defaultValue": "black",
            "selectors": [".form-title"]
          },
          {
            "type": "item",
            "behaviour": "color",
            "title": "Text #2",
            "defaultValue": "white",
            "selectors": ["h1", "h2"]
          }
        ]
      },
      {
        "type": "group",
        "title": "Interactive components",
        "elements": [
          {
            "type": "item",
            "behaviour": "color-background",
            "title": "Button",
            "defaultValue": "#0057FF",
            "selectors": [".form-action button"]
          }
        ]
      }
    ]
  }
]
